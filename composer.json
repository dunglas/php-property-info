{
    "name": "dunglas/php-property-info",
    "type": "library",
    "description": "Retrieve type and description of PHP properties using various sources ",
    "keywords": [
        "property",
        "type",
        "PHPDoc",
        "symfony",
        "validator",
        "doctrine"
    ],
    "homepage": "https://dunglas.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "dunglas@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "PropertyInfo\\": "src/PropertyInfo"
        }
    },
